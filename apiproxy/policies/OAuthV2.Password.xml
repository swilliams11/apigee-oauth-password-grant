<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 name="OAuthV2.Password" continueOnError="true">
    <Operation>GenerateAccessToken</Operation>
    <ExpiresIn>1200000</ExpiresIn>
    <SupportedGrantTypes>
        <GrantType>password</GrantType>
    </SupportedGrantTypes>
    <GrantType>flow.request.grantType</GrantType>
    <Attributes>
        <Attribute name="username" ref="flow.login.username"/>
        <Attribute name="enterpriseid" ref="flow.login.enterpriseid"/>
    </Attributes>
    <UserName>flow.request.username</UserName>
    <PassWord>flow.request.password</PassWord>
    <GenerateResponse enabled="true"/>
</OAuthV2>
